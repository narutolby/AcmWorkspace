#include<stdio.h>
int main(){
	char*input[8]={0},i;short n=0;
	input[0]="mail.google.com";
	input[1]="www.sina.com.cn";
	input[2]="www.sohu.com";
	input[3]="mail.bupt.edu.cn";
	input[4]="www.baidu.com";
	input[5]="www.zol.com.cn";
	input[6]="acm.scs.bupt.cn";
	input[7]="acm.fzu.edu.cn";
	scanf("%c",&i);
	
	for(n=0;n<8;n++){
		if(*input[n]==i){
            printf("%s\n",input[n]);
		}
	}

	return 0;
}
